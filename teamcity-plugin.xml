<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>elastic-timeout</name>
    <display-name>elastic-timeout</display-name>
    <version>@Version@</version>
    <description>A Teamcity timeout plugin which takes the build history into account</description>
    <download-url>https://github.com/foeser/elastic-timeout/releases</download-url>
    <email>florian.oeser(at)gmail.com</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo>Plugin vendor logo URL</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true"/>
</teamcity-plugin>
